{
    "compilerOptions": {
        "target": "ES2024",
        "module": "ES2015",
        "lib": [
            "dom",
            "es2021",
            "scripthost"
        ],
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "allowJs": true,
        "downlevelIteration": true,
        "esModuleInterop": true,
        "preserveSymlinks": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "useUnknownInCatchVariables": false
    },
    "include": ["src"]
}